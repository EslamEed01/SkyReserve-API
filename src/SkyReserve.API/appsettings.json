{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "SkyReserve.Infrastructure.Persistence.QueryTimingInterceptor": "Information"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5433;Database=mydb;Username=admin;Password=admin123"
  },

  "Jwt": {
    "Key": "",
    "Issuer": "SkyReserve",
    "Audience": "SkyReserve users",
    "ExpiryMinutes": 60
  },
  "Stripe": {
    "SecretKey": "",
    "PublishableKey": ""
  },

  "AwsSes": {
    "AccessKey": "",
    "SecretKey": "",
    "Region": "us-east-1",
    "FromEmail": "no-reply@eslameed.app",
    "FromName": "SkyReserve",
    "ReplyToEmail": "support@eslameed.app",
    "UseSandbox": true
  },
  "Twilio": {
    "AccountSid": "",
    "Secret": "",
    "FromPhone": "+12184384157"
  },

  "AwsSqs": {
    "AccessKey": "",
    "SecretKey": "",
    "Region": "us-east-1",
    "QueueUrl": "https://sqs.us-east-1.amazonaws.com/452529558059/SkyReserveNotificationsQueue",
    "DeadLetterQueueUrl": "https://sqs.us-east-1.amazonaws.com/452529558059/SkyReserveDeadLetterQueue",
    "MaxMessages": 10,
    "WaitTimeSeconds": 20,
    "VisibilityTimeoutSeconds": 30,
    "MaxReceiveCount": 3,
    "EnableDeadLetterQueue": true,
    "MessageRetentionPeriod": 1209600
  },

  "Elasticsearch": {
    "Url": "http://localhost:9201",
    "DefaultIndex": "flights"
  },
  "Kibana": {
    "Uri": "http://localhost:5601"
  },

  "Redis": {
    "ConnectionString": "localhost:6379",
    "KeyPrefix": "SkyReserve:",
    "Database": 0,
    "DefaultExpiration": "01:00:00"
  },

  "Grafana": {
    "Url": "http://localhost:3000",
    "Username": "admin",
    "Password": "admin",
    "Security": {
      "EnableHttps": false
    }
  },

  "GoogleOAuth": {
    "ClientId": "",
    "ClientSecret": "",
    "RedirectUri": "https://localhost:5001/google/callback",
    "Scopes": [
      "openid",
      "profile",
      "email",
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/calendar.events"
    ],
    "AuthorizationEndpoint": "https://accounts.google.com/o/oauth2/v2/auth",
    "TokenEndpoint": "https://oauth2.googleapis.com/token",
    "UserInfoEndpoint": "https://www.googleapis.com/oauth2/v2/userinfo"
  },

  "GoogleCalendar": {
    "ApiBaseUrl": "https://www.googleapis.com/calendar/v3",
    "DefaultCalendarId": "primary",
    "TimeZone": "UTC"
